{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@services/*":["./services/*"],
      "@app/*":["./app/*"],
      "@components/*":["./components/*"],
      "@constants/*":["./constants/*"],
      "@redux/*":["./redux/*"],
      "@script/*":["./script/*"],
      "@assets/*":["./assets/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}