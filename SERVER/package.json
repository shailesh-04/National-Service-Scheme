{
    "name": "nss-project-backend",
    "version": "1.0.0",
    "description": "The National Sevice Scheme (NSS) Android App BackEnd Project Apis",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node src/index.js",
        "build": "tsc",
        "start": "node dist/index.js",
        "clean": "rm -rf dist",
        "test": "node temp.test.js",
        "migration": "node ./src/database/migrations/_migrations.js",
        "create-migration": "node ./src/scripts/createMigratoin.js"
    },
    "imports": {
        "#src/*": "./src/*",
        "#color": "./src/services/color.js",
        "#controllers/*": "./src/controllers/*",
        "#models/*": "./src/database/models/*",
        "#migrations/*": "./src/database/migrations/*",
        "#config/*": "./config/*",
        "#routes/*": "./src/routes/*",
        "#middleware/*": "./src/middleware/*",
        "#services/*": "./src/services/*",
        "#untils/*": "./src/untils/*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shailesh-04/NSS.git"
    },
    "keywords": [
        "NSS"
    ],
    "author": "shailesh makavana",
    "license": "ISC",
    "dependencies": {
        "cloudinary": "^1.41.3",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "ejs": "^3.1.10",
        "expo-server-sdk": "^3.14.0",
        "express": "^4.21.2",
        "express-session": "^1.18.1",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "multer-storage-cloudinary": "^4.0.0",
        "mysql2": "^3.12.0",
        "nodemailer": "^6.10.0",
        "socket.io": "^4.8.1"
    },
    "devDependencies": {
        "@types/node": "^22.14.1",
        "nodemon": "^3.1.9",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
